<resources>
    <style name="AppTheme" parent="@style/WhiteTheme"></style>
    <!-- Base application theme. -->
    <style name="WhiteTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <!-- 状态栏颜色，会被statusBarColor效果覆盖-->
        <item name="colorPrimaryDark">@color/start</item>
        <!-- 状态栏颜色，继承自colorPrimaryDark -->
        <item name="android:statusBarColor">@color/start</item>
        <!-- actionBar颜色 -->
        <item name="colorPrimary">@color/start</item>
        <!-- 背景颜色 -->
        <item name="android:windowBackground">@color/start</item>
        <!-- 底部栏颜色 -->
        <item name="android:navigationBarColor">@color/start</item>

        <!-- ListView的分割线颜色，switch滑动区域色-->
        <item name="android:colorForeground">@color/start</item>
        <!-- popMenu的背景色 -->
        <item name="android:colorBackground">@color/start</item>

        <!-- 控件默认颜色 ，效果会被colorControlActivated取代  -->
        <item name="android:colorAccent">@color/start</item>

        <!-- 控件默认时颜色  -->
        <item name="android:colorControlNormal">@color/start</item>
        <!-- 控件按压时颜色，会影响水波纹效果，继承自colorAccent  -->
        <item name="android:colorControlHighlight">@color/start</item>
        <!-- 控件选中时颜色 -->
        <item name="android:colorControlActivated">@color/start</item>
        <!-- Button的默认背景 -->
        <item name="android:colorButtonNormal">@color/start</item>

        <!-- Button，textView的文字颜色  -->
        <item name="android:textColor">@color/start</item>
        <!-- RadioButton checkbox等控件的文字 -->
        <item name="android:textColorPrimaryDisableOnly">@color/start</item>
        <!-- actionBar的标题文字颜色 -->
        <item name="android:textColorPrimary">@color/start</item>


    </style>


